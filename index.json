{
  "$schema": "http://json.schemastore.org/prettierrc",
  "singleQuote": true,
  "semi": false,
  "tabWidth": 2,
  "useTabs": false,
  "printWidth": 80,
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "proseWrap": "always",
  "endOfLine": "lf",
  "trailingComma": "all",
  "jsxBracketSameLine": false,
  "jsxSingleQuote": false,
  "htmlWhitespaceSensitivity": "css",
  "quoteProps": "as-needed"
}
